{
  "name": "amc-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:host": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write --cache \"**/*.{js,ts,svelte,json,md,css,html}\"",
    "lint": "prettier --check --cache \"**/*.{js,ts,svelte,json,md,css,html}\" && eslint \"**/*.{js,ts,svelte}\"",
    "lint:fix": "eslint --fix \"**/*.{js,ts,svelte}\"",
    "test:unit": "vitest",
    "test": "vitest run",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "paraglide:compile": "pnpm exec paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
    "checklist": "pnpm run format && pnpm run lint && pnpm run paraglide:compile && pnpm run check && pnpm run test"
  },
  "devDependencies": {
    "@eslint/compat": "1.4.0",
    "@eslint/js": "9.38.0",
    "@iconify-json/material-symbols": "1.2.42",
    "@storybook/addon-svelte-csf": "5.0.10",
    "@storybook/addon-themes": "9.1.13",
    "@storybook/sveltekit": "9.1.13",
    "@sveltejs/adapter-static": "3.0.10",
    "@sveltejs/kit": "2.47.3",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@testing-library/jest-dom": "6.9.1",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.17.0",
    "@unocss/extractor-svelte": "66.5.4",
    "@vitest/browser": "4.0.1",
    "@vitest/browser-playwright": "4.0.1",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-storybook": "9.1.13",
    "eslint-plugin-svelte": "3.12.5",
    "globals": "16.4.0",
    "playwright": "1.56.1",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "prettier-plugin-tailwindcss": "0.7.1",
    "react": "19.2.0",
    "storybook": "9.1.13",
    "svelte": "5.41.2",
    "svelte-check": "4.3.3",
    "terser": "5.44.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "unocss": "66.5.4",
    "vite": "7.1.11",
    "vite-bundle-analyzer": "1.2.3",
    "vite-plugin-devtools-json": "1.0.0",
    "vitest": "4.0.1",
    "vitest-browser-svelte": "2.0.0"
  },
  "dependencies": {
    "@inlang/paraglide-js": "2.4.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "dompurify": "3.3.0",
    "gsap": "3.13.0",
    "lodash-es": "4.17.21",
    "lottie-web": "5.13.0",
    "marked": "16.4.1",
    "ol": "10.6.1",
    "quaternion": "2.2.0",
    "svelte-portal": "2.2.1",
    "tailwind-merge": "3.3.1",
    "zod": "4.1.12"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
