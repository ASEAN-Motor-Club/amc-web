{
  "name": "amc-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:host": "vite dev --host",
    "build": "vite build && node scripts/remove-unused-css-vars.js",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write --cache .",
    "lint": "prettier --check --cache . && eslint --cache --cache-location \"node_modules/.cache/.eslintcache\" --cache-strategy content .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "paraglide:compile": "npx @inlang/paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
    "checklist": "npm run format && npm run lint && npm run paraglide:compile && npm run check && npm run test",
    "check:uno-classes": "npm run build && node scripts/find-non-uno-classes.js"
  },
  "devDependencies": {
    "@eslint/compat": "1.3.1",
    "@eslint/js": "9.31.0",
    "@iconify-json/material-symbols": "1.2.29",
    "@storybook/addon-svelte-csf": "5.0.7",
    "@storybook/addon-themes": "9.0.17",
    "@storybook/sveltekit": "9.0.17",
    "@sveltejs/adapter-static": "3.0.8",
    "@sveltejs/kit": "2.23.0",
    "@sveltejs/vite-plugin-svelte": "6.0.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/svelte": "5.2.8",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.16.4",
    "@unocss/svelte-scoped": "66.3.3",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-storybook": "9.0.17",
    "eslint-plugin-svelte": "3.10.1",
    "glob": "11.0.3",
    "globals": "16.3.0",
    "jsdom": "26.1.0",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "react": "19.1.0",
    "storybook": "9.0.17",
    "svelte": "5.36.0",
    "svelte-check": "4.2.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.37.0",
    "unocss": "66.3.3",
    "vite": "7.0.4",
    "vite-plugin-devtools-json": "0.3.0",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@inlang/paraglide-js": "2.2.0",
    "clsx": "2.1.1",
    "gsap": "3.13.0",
    "lodash-es": "4.17.21",
    "lottie-web": "5.13.0",
    "mt-map": "github:beam41/mt-map",
    "ol": "10.6.1",
    "quaternion": "2.0.2",
    "svelte-portal": "2.2.1",
    "zod": "4.0.5"
  }
}
