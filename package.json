{
  "name": "amc-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:host": "vite dev --host",
    "build": "vite build && node scripts/remove-unused-css-vars.js",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "paraglide:compile": "npx @inlang/paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
    "checklist": "npm run format && npm run lint && npm run check && npm run test"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@iconify-json/material-symbols": "^1.2.29",
    "@storybook/addon-svelte-csf": "^5.0.3",
    "@storybook/addon-themes": "^9.0.5",
    "@storybook/sveltekit": "^9.0.5",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.4",
    "@types/glob": "^8.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22",
    "@unocss/svelte-scoped": "^66.3.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-storybook": "^9.0.5",
    "eslint-plugin-svelte": "^3.0.0",
    "glob": "^11.0.3",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "react": "^19.1.0",
    "storybook": "^9.0.5",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "unocss": "^66.3.2",
    "vite": "^6.2.6",
    "vite-plugin-devtools-json": "^0.2.1",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@inlang/paraglide-js": "^2.0.0",
    "clsx": "^2.1.1",
    "lodash-es": "^4.17.21",
    "lottie-web": "^5.13.0",
    "mt-map": "github:beam41/mt-map",
    "ol": "^10.6.1",
    "quaternion": "^2.0.2",
    "svelte-portal": "^2.2.1",
    "zod": "^3.25.57"
  }
}
